%h1 List of All Paintings
- if @paintings.any?
	%table.museum
		%tbody
			%tr
				%th Museum
				%th Painting
				%th Year 
				%th Artist
				%th Image
			= render partial: "paintings/painting", collection: @paintings
- else
	%h2 No paintings

= link_to "Add New Painting", new_painting_path, class: "btn btn-yellow btn-small"

= render 'static_pages/home_links'